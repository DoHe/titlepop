{
    "manifest_version": 2,
    "name": "TitlePop",
    "version": "1.1",
    "description": "__MSG_extensionDescription__",
    "default_locale": "en",
    "icons": {
        "48": "icons/48.png",
        "96": "icons/96.png"
    },
    "permissions": [
        "activeTab",
        "menus",
        "storage"
    ],
    "background": {
        "scripts": [
            "background/init.js"
        ]
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content/pop.js"
            ],
            "css": [
                "content/modal.css"
            ],
            "run_at": "document_idle"
        }
    ],
    "options_ui": {
        "page": "settings/settings.html",
        "open_in_tab": true,
        "browser_style": false
    },
    "browser_specific_settings": {
        "gecko": {
            "id": "{5acb2c0f-7943-4c9d-b8d3-8e3bbf4157b8}"
        }
    }
}