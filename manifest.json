{
    "manifest_version": 2,
    "name": "TitlePop",
    "version": "1.1",
    "description": "__MSG_extensionDescription__",
    "default_locale": "en",
    "icons": {
        "48": "icons/48.png",
        "96": "icons/96.png"
    },
    "permissions": [
        "activeTab",
        "menus"
    ],
    "background": {
        "scripts": [
            "scripts/background.js"
        ]
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "scripts/content.js"
            ]
        }
    ]
}